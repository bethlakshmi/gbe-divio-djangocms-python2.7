{% extends 'gbe/gbe_wizard.tmpl' %}
{% block intro %}
<h2>Technical Info for {{act.b_title}}</h2>
{% for show in shows %}
<b>Booked for: {{show}}</b>
{% endfor %}
<p><b>Submitted Description: </b>{{act.b_description}}</p>
{% endblock %}
{% block first_form %}
<p>{{ basic_instructions }}</p>
<form action="" method="post" enctype="multipart/form-data">
{% include "form_table_wrapper.tmpl" with forms=basic_forms %}
  <div class="form-row pull-right">
  <input type="submit" name="submit" value="Book Rehearsal & Save Basics">
  </div>
</form>
{% endblock %}
